echo "runbfi"
# cc bfi.c -I.. -L../.libs -DBSDXATTRS -l sqlite3 -o bfi
cc bfi.c -pthread -l sqlite3 -o bfi
rm -rf testdirdup1
mkdir testdirdup1
./bfi testdir testdirdup1 1 5
ls -ld testdir
ls -ld testdirdup1/testdir
ls -ld testdir/c
ls -ld testdirdup1/testdir/c
ls -ld testdir/c/cc
ls -ld testdirdup1/testdir/c/cc
#xattr -l testdir/c/cc
attr -l testdir/c/cc
#xattr -l testdirdup1/testdir/c/cc
attr -l testdirdup1/testdir/c/cc

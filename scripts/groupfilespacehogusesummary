EPATH=..
echo "query to create db insert recs from entry query into out db group file space hog use group summary"
rm -f outdb*
$EPATH/bfq -i $1 -E "insert into sumsum select gidtouser(gid), totfiles, totsize from vsummarygroup;" -n $2 -O outdb -I "create table sumsum (groupname text, totfiles int64, totsize int64);"
$EPATH/querydbn outdb "select groupname, totfiles, totsize from vsumsum group by groupname;" 0 1 1 $2 sumsum

EPATH=..
echo "query to create db insert recs from entry query into out db user file space hog"
rm -f outdb*
$EPATH/bfq -i $1 -E "insert into sument select uidtouser(uid), count(*), sum(size) from entries where type='f' group by uid;" -n $2 -O outdb -I "create table sument (username text, totfiles int64, totsize int64);"
$EPATH/querydbn outdb "select username, totfiles, totsize from vsument group by username;" 0 1 1 $2 sument

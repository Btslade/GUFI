EPATH=..
echo "query to create db insert recs from entry query into out db group file space hog"
rm -f outdb*
$EPATH/bfq -i $1 -E "insert into sument select gidtogroup(gid), count(*), sum(size) from entries where type='f' group by gid;" -n $2 -O outdb -I "create table sument (groupname text, totfiles int64, totsize int64);"
$EPATH/querydbn outdb "select groupname, totfiles, totsize from vsument group by groupname;" 0 1 1 $2 sument

time ./bfti /tmp/testdirdup2/tmp/bigtestdir  0 40 0

echo "---> select on summary and on entries filed bigger than 5"
./bfq /tmp/testdirdup2/tmp/bigtestdir "" "select name,type,uid,gid,size from summary" "select name,type,uid,gid,size from entries where size > 5" 0 0 0 20 0
echo "---> no select on summary and select on entries filed bigger than 5"
./bfq /tmp/testdirdup2/tmp/bigtestdir "" "" "select name,type,uid,gid,size from entries where size > 5" 0 0 0 20 0
echo "---> no select on summary and select on entries name like 600010"
./bfq /tmp/testdirdup2/tmp/bigtestdir "" "" "select name,type,uid,gid,size from entries where name like '%600010%'" 0 0 0 20 0
echo "---> select on summary files bigger than 5 and select on entries ize bigger than 5"
./bfq /tmp/testdirdup2/tmp/bigtestdir "" "select name,type,uid,gid,size from summary where size > 5" "select name,type,uid,gid,size from entries where size > 5" 0 0 0 20 0
echo "---> select on summary files bigger than 5 and select on entries size bigger than 5 printing pinode"
./bfq /tmp/testdirdup2/tmp/bigtestdir "" "select name,type,inode,uid,gid,size from summary where size > 5" "select name,type,inode,uid,gid,size from entries where size > 5" 0 0 0 20 0
echo "---> select on treesummary where maxsixe > 10000000 and select select on summary files bigger than 10000000 and select on entries size bigger than 10000000 printing pinode"
./bfq /tmp/testdirdup2/tmp/bigtestdir "select maxsize from treesummary where maxsize > 100000000" "select name,type,inode,uid,gid,size from summary where maxsize > 100000000" "select name,type,inode,uid,gid,size from entries where size > 100000000" 0 0 0 40 0

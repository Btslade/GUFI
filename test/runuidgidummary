echo "delete uid and gid summaries"
../scripts/deluidgidsummaryrecs	testdirdup/testdir
echo "generate uid and gid summaries"
../scripts/generategidsummary testdirdup/testdir
../scripts/generateuidsummary testdirdup/testdir
echo "list uid summaries"
../bfq -i testdirdup/testdir -S "select epath(),uid,totfiles,totsize from summary where rectype=1;" -Pp -n 1
echo "list gid summaries"
../bfq -i testdirdup/testdir -S "select epath(),gid,totfiles,totsize from summary where rectype=2;" -Pp -n 1
